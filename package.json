{
  "name": "prochat",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=.;NODE_ENV=production node ./bin/www",
    "coveragenyc": "nyc --check-coverage --lines 100 npm run coverage",
    "lint": "eslint *.js test/",
    "test": "NODE_PATH=.;NODE_ENV=test mocha --compilers js:babel-register",
    "coverage": "rm -rf coverage/* && NODE_PATH=.;NODE_ENV=test babel-node ./node_modules/.bin/istanbul cover _mocha -- --compilers js:babel-register",
    "dev": "NODE_PATH=.;NODE_ENV=development nodemon ./bin/www --exec babel-node",
    "compile": "rm -rf dist/* && babel ./src -d dist"
  },
  "dependencies": {
    "async": "^2.4.1",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "body-parser": "~> 1.16.0",
    "connect": "^3.6.2",
    "connect-mongo": "~> 1.3.2",
    "cookie": "~> 0.3.1",
    "cookie-parser": "~> 1.4.3",
    "cool-ascii-faces": "~> 1.3.4",
    "crypto": "~> 0.0.3",
    "debug": "^2.6.8",
    "ejs": "~> 2.5.5",
    "ejs-locals": "~> 1.0.2",
    "errorhandler": "~> 1.5.0",
    "express": "~> 4.14.1",
    "express-jquery": "~> 1.8.3",
    "express-session": "^1.15.3",
    "handlebars": "^4.0.10",
    "mongodb": "~> 2.2.24",
    "mongoose": "~> 4.8.4",
    "morgan": "~> 1.7.0",
    "nconf": "~> 0.8.4",
    "serve-favicon": "~> 2.3.2",
    "socket.io": "~> 1.7.4",
    "winston": "~> 2.3.1"
  },
  "devDependencies": {
    "casperjs": "^1.1.4",
    "chai": "^3.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.4.1",
    "nodemon": "^1.10.0",
    "nyc": "^10.3.2"
  }
}
